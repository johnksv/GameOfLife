<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox id="vboxWavRoot" prefHeight="311.0" prefWidth="352.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gol.s305089.controller.WavMakerController">
    <children>
        <Label text="Generate WAV" />
      <Label text="Iterations" />
      <Spinner fx:id="spinnIte" />
        <Label text="Duration of each iteration (in seconds)" />
        <Spinner fx:id="spinnDur" />
        <Label text="Base/root tone" />
        <ComboBox prefWidth="150.0" />
        <HBox spacing="20.0">
            <children>
                <Button mnemonicParsing="false" text="Preview" />
                <Button mnemonicParsing="false" onAction="#saveWav" text="Save as Wav" />
            </children>
            <VBox.margin>
                <Insets top="10.0" />
            </VBox.margin>
        </HBox>
    </children>
</VBox>
